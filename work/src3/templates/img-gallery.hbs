{{#each this}}
{{!-- <li class="gallery-item">
    <div class="photo-card">

        <img src="{{webformatURL}}" class="photo" alt={{tags}} data-source="{{largeImageURL}}" /></a> --}}



        <div class="photo-card">
            <img src="{{webformatURL}}" alt="{{largeImageURL}}" />
            {{!-- webformatURL - ссылка на маленькое изображение для списка карточек
            largeImageURL - ссылка на большое изображение (смотри пункт 'дополнительно') --}}

            <div class="stats">
                <p class="stats-item">
                    <i class="material-icons">thumb_up</i>
                    {{likes}}
                    {{!-- 1108 - количество лайков --}}
                </p>
                <p class="stats-item">
                    <i class="material-icons">visibility</i>
                    {{views}}
                    {{!-- 320321 - количество просмотров --}}
                </p>
                <p class="stats-item">
                    <i class="material-icons">comment</i>
                    {{comments}}
                    {{!-- 129 - количество комментариев --}}
                </p>
                <p class="stats-item">
                    <i class="material-icons">cloud_download</i>
                    {{downloads}}
                    {{!-- 176019 - количество загрузок --}}
                </p>
            </div>
        </div>

        {{/each}}